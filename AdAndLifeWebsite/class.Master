                </div>
                <div class="menu-item">
                    <a href="/articles.aspx" class="menu-item-text<%= _isSel("articles.aspx") %>">Статьи</a>
                </div>
                <%if (AdAndLifeWebsite.Models.TicketsSale.AllForSale.Count() > 0)
